{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=/data/app.db;Mode=ReadWriteCreate"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BackupPath": "/backups",
  "HangfireDbPath": "/data/hangfire.db",
  "dayBeforeCompression": 2,
  "cronForCompressionJob": "30 0 */1 * *",
  "MailSettings": null
}